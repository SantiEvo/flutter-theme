    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_theme",
            "request": "launch",
            "type": "dart",
            "env": {
                "THEME_ID": "custom_theme_2"
            }
        },
        {
            "name": "flutter_theme (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "env": {
                "THEME_ID": "custom_theme_1"
            }
        },
        {
            "name": "flutter_theme (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "env": {
                "THEME_ID": "custom_theme_3"
            }
        }
    ]
}